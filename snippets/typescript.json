{
  "test": {
      "prefix": "tt",
      "body": [
        "test(`${1:description}`, async ({}) => {",
        "",
        "})"
      ],
      "description": "Test"
  },

  "test step": {
      "prefix": "tts",
      "body": [
        "await test.step(`${1:description}`, async () => {",
        "",
        "})"
      ],
      "description": "Test Step"
  },

  "named test step": {
      "prefix": "ttc",
      "body": [
        "await ${1:name}.step(`${2:description}`, async () => {",
        "\tawait ${3:step}",
        "})"
      ],
      "description": "Named Test Step"
  },

  "test expect": {
      "prefix": "tte",
      "body": [
        "await expect(${1:expected}).to${2}"
      ],
      "description": "Test Step Expect"
  },

  "create asset": {
    "prefix": "ca",
    "body": [
      "const uid = shortid()",
      "const assetName = `Asset \\${uid}`",
      "",
      "await test.step(`Create Print ${assetName}`, async () => {",
      "\tawait api.createAsset({",
      "\t\t...Mutables.DEFAULT_ASSET.TEMPLATE.ACTIVE_PRINT_TEMPLATE,",
      "\t\t\"name\": assetName,",
      "\t\t\"assetType\": { \"id\": Constants.ASSET_TYPES.PRINT_TEMPLATE.id }",
      "\t})",
      "})"
    ],
    "description": "Create Asset"
  },

  "create configurable asset": {
    "prefix": "cca",
    "body": [
      "const uid = shortid()",
      "const assetName = `Asset \\${uid}`",
      "",
      "await test.step(`Create Print \\${assetName}`, async () => {",
      "\tconst _asset = await api.createAsset({",
      "\t\t...Mutables.DEFAULT_ASSET.TEMPLATE.ACTIVE_PRINT_TEMPLATE,",
      "\t\t\"name\": assetName,",
      "\t\t\"assetType\": { \"id\": Constants.ASSET_TYPES.PRINT_TEMPLATE.id }",
      "\t})",
      "",
      "\tif (!_asset) throw new Error('Failed to create asset')",
      "\tawait api.updatePrintTemplate(_asset, definitions.defaultPrintTemplate)",
      "})"
    ],
    "description": "Create Asset"
  },

  "search for asset": {
    "prefix": "sfa",
    "body": [
      "await test.step(`Search for asset`, async () => {",
      "\tawait home.search($1)",
      "\tawait expect(async () => {",
      "\t\tawait assetLibrary.page.reload()",
      "\t\tif (await assetLibrary.sortingHeader.refreshResults.isVisible()) {",
      "\t\t\tawait assetLibrary.sortingHeader.refreshResults.click()",
      "\t\t}",
      "\t\tawait expect(assetLibrary.tile($2).component).toBeVisible()",
      "\t}).toPass({ intervals: [3000, 7000, 11000] })",
      "})"
    ],
    "description": "Search for asset"
  },

  "start test file": {
    "prefix": "stt",
    "body": [
      "import { test } from '@fixtures/index'",
      "",
      "test.describe('Describe', () => {",
      "",
      "\ttest.beforeEach(async ({ login }) => { login })",
      "",
      "\ttest('Test', async ({}) => {",
      "",
      "\t})",
      "",
      "})"
    ],
    "description": "Start Test File"
  }

}
